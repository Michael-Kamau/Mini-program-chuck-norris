<view>

  <picker onChange="bindCategoriesPickerChange" value="{{selectedCategoryIndex}}" range="{{categories}}" range-key="name">
    <view class="row">
      <view class="row-title">Select Joke category</view>
      <view class="row-extra">Current selection:{{categories[selectedCategoryIndex]}}</view>
      
    </view>
  </picker>
  <view>{{joke}}</view>

  <button onTap="fetchCategoryJoke">Fetch Joke</button>


</view>
